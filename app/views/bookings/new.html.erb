<div class="container">

<h1>Book this Garden</h1>

<%= simple_form_for([@garden, @booking], html: {method: :post}) do |f| %>
  <div class="form-inputs">
    <%= f.input :start_date, as: :date %>
  </div>
  <div class="form-inputs">
    <%= f.input :end_date, as: :date %>
  </div>

  <div class="form-actions">
    <%= f.submit  "submit my garden", class: "btn btn-success" %>
  </div>
<% end %>
</div>
